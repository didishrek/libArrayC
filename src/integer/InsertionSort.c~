#include	"IntegerArray.h"

int		InsertionSort(int find, int *tab, size_t size)
{
  size_t	i;
  size_t	j;
  int		x;
  
  i = 1;
  while (i < size -1)
    {
      x = tab[i];
      j = i;
      while (j > 0 && tab[j - 1] > x)
	{
	  tab[j] = tab[j - 1];
	  j--;
	}
      tab[j] = x;
      i++;
    }
}
